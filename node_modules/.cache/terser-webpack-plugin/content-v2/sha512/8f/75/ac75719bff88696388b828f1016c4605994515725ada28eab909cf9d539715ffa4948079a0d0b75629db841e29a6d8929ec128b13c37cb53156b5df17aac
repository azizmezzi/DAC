          data={FooderListe2}\\n              title={\\n                this.state.Role < 1 ? (\\n                  <Row>\\n                    <center>\\n                      <a\\n                        onClick={() => this.setState({ isPaneOpenLeft: true })}\\n                        className=\\\"AddButton\\\"\\n                      >\\n                        <center>\\n                          Aliments &nbsp; &nbsp;\\n                          <FontAwesomeIcon size=\\\"lg\\\" icon={faPlus} />{' '}\\n                        </center>\\n                      </a>\\n                    </center>\\n                  </Row>\\n                ) : (\\n                  'Aliments'\\n                )\\n              }\\n              options={{\\n                pageSize: 10,\\n                maxBodyHeight: '46vh',\\n                columnsButton: true\\n              }}\\n            />\\n          </Col>\\n          <NotificationContainer />\\n\\n          <SlidingPane\\n            closeIcon={\\n              <div>\\n                <center>Ajouter un Regime</center>\\n              </div>\\n            }\\n            isOpen={this.state.isPaneOpenLeft}\\n            title=\\\"\\\"\\n            from=\\\"left\\\"\\n            width=\\\"58%\\\"\\n            onRequestClose={() => this.setState({ isPaneOpenLeft: false })}\\n          >\\n            <Row>\\n              <Col lg=\\\"4\\\" md=\\\"6\\\" sm=\\\"12\\\" className=\\\"mb-4\\\" />\\n              <Col lg=\\\"8\\\" md=\\\"6\\\" sm=\\\"12\\\" className=\\\"mb-4\\\">\\n                {this.renderActionF2()}\\n              </Col>\\n            </Row>\\n          </SlidingPane>\\n        </Row>\\n\\n        {/* Default Light Table */}\\n      </Fragment>\\n    );\\n  };\\n\\n  renderActionF2 = () => {\\n    return (\\n      <Fragment>\\n        {/* Page Header */}\\n\\n        <Row>\\n          <h4> Ajouter Aliment</h4>\\n        </Row>\\n        <br />\\n        <br />\\n        <br />\\n\\n        <FormF